{
  "name": "ai-tarot-decision-assistant",
  "version": "1.0.0",
  "description": "AI-powered tarot decision assistant with 78 cards, achievements, analytics, and premium subscriptions",
  "main": "src/backend/index-json.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/backend/index-json.js",
    "client:dev": "cd src/frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd src/frontend && npm run build",
    "build:render": "npm --prefix src/frontend install && npm --prefix src/frontend run build",
    "start": "NODE_ENV=production node src/backend/index-json.js",
    "start:prod": "NODE_ENV=production node src/backend/index-json.js",
    "docker:build": "docker build -t tarot-assistant .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "db:seed": "node src/backend/scripts/seed-cards.js",
    "icons:generate": "node scripts/generate-pwa-icons.js",
    "production:check": "node scripts/production-check.js",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "deploy:check": "node scripts/deployment-check.js",
    "deploy:vercel": "cd src/frontend && vercel --prod",
    "deploy:railway": "railway up",
    "backup": "node scripts/create-backup.js",
    "test:comprehensive": "node scripts/comprehensive-test.js"
  },
  "keywords": [
    "tarot",
    "decision-making",
    "wellness",
    "mindfulness",
    "stripe",
    "premium",
    "gamification",
    "analytics",
    "pwa",
    "russian"
  ],
  "author": "AI Tarot Team",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-tarot-decision-assistant"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-tarot-decision-assistant/issues"
  },
  "homepage": "https://tarot-assistant.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@sentry/node": "^10.28.0",
    "@sentry/profiling-node": "^10.28.0",
    "@supabase/supabase-js": "^2.86.2",
    "astronomia": "^4.2.0",
    "aws-sdk": "^2.1511.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "html-pdf-node": "^1.0.8",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.14.0",
    "redis": "^4.6.11",
    "sharp": "^0.33.1",
    "stripe": "^14.25.0",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  }
}
